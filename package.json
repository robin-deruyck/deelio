{
  "name": "deelio",
  "version": "0.0.1",
  "description": "Model driven express plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**"],
  "scripts": {
    "dev": "tsup src/lib/index.ts --watch ",
    "build": "tsup --minify --dts",
    "test": "vitest dev",
    "test:ui": "vitest --ui --coverage",
    "test:ci": "vitest -w false --coverage",
    "pretty": "prettier --write \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "pkg:publish": "pnpm test:ci && pnpm build && pnpm pack --pack-destination dist && pnpm publish dist"
  },
  "keywords": [],
  "author": "@robin-deruyck",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "express": "^4.18.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "deelio": "link:"
  }
}
